{
  "name": "enterprise-nx-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "bundlesize": [
    {
      "path": "./dist/apps/web-app/**/*.js",
      "maxSize": "300 kB"
    }
  ],
  "scripts": {
    "build": "nx build web-app",
    "build:all": "nx run-many --target=build --all --parallel",
    "test": "nx test web-app",
    "test:all": "nx run-many --target=test --all",
    "test:web-app": "nx test web-app",
    "test:fastify-api": "nx test fastify-api",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "e2e": "nx e2e e2e",
    "e2e:headless": "nx e2e e2e --headless",
    "dev": "nx run-many --target=serve --projects=web-app,fastify-api --parallel",
    "dev:all": "pnpm run dev",
    "serve:web-app": "nx serve web-app",
    "serve:fastify-api": "nx serve fastify-api",
    "build:web-app": "nx build web-app",
    "build:fastify-api": "nx build fastify-api",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:db": "./scripts/dev-db-setup.sh",
    "docker:clean": "./scripts/dev-db-teardown.sh",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "storybook": "nx storybook web-app",
    "lint": "nx lint",
    "lint:all": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "nx format",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "type-check": "nx run-many --target=type-check --all",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "ci": "pnpm run lint:all && pnpm run type-check && pnpm run test:coverage && pnpm run build:all",
    "security:audit": "pnpm audit --audit-level moderate",
    "security:check": "pnpm run security:audit",
    "graph": "nx graph",
    "reset": "nx reset"
  },
  "private": true,
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/static": "^8.2.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.1",
    "fastify": "^5.4.0",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router-dom": "6.29.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.3.11",
    "@nx/esbuild": "^21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/node": "^21.3.11",
    "@nx/playwright": "21.3.11",
    "@nx/react": "21.3.11",
    "@nx/storybook": "^21.3.11",
    "@nx/vite": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@playwright/test": "^1.54.2",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.9",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vanilla-extract/webpack-plugin": "^2.3.22",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
